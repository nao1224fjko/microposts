<!-- editアクションが行われる際に下記が表示される。どこから入ってきてもいいようにアクション名をeditもupdateも両方ちゃんと書いておく -->
<% if params[:action] == 'edit' || params[:action] == 'update' %>
    <br>
    <%= f.label :nickname %>
    <%= f.text_field :nickname, class: 'form-control' %>
    
    <%= f.label :birthday %>
    <%= f.date_field :birthday, class: 'form-control' %>
<% end %>